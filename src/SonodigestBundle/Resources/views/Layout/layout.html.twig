        <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>
        <script src="{{ asset('bundles/adesignscalendar/js/fullcalendar/jquery.fullcalendar.min.js') }}"></script>
        <script src="{{ asset('Resources/jquery-ui/ui/jquery-ui.js') }}"></script>
        <!-- fullcalendar -->
        
        <!-- COMPONENTS -->
        <script src="{{ asset('Resources/ptTimeSelect/js/jquery.ptTimeSelect.js') }}"></script>  
        <!-- END COMPONENTS -->
        
        <script src="{{ asset('Resources/bootstrap-3/js/bootstrap.min.js') }}"></script> 
        
        <script type="text/javascript" src="{{ asset('Resources/bValidator-0.73/jquery.bvalidator.js')}}"></script>
        <script type="text/javascript" src="{{ asset('Resources/bValidator-0.73/bvalidator.lang.es.js')}}" ></script>
        <script type="text/javascript" src="{{ asset('Resources/zebra/js/zebra_datepicker.js')}}" ></script>
        <script src="{{ asset('Resources/wrapkit/scripts/fileinput.js') }}"></script>
