<!DOCTYPE html>
<html class="no-js" lang="en">
    <head>
    {%block head -%} 
        {% block title -%}<title>:{% block titlename -%}::: La Plus Belle :::{% endblock %}</title>{% endblock %}
        {% block meta -%}
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <meta name="viewport" content="initial-scale=1,minimum-scale=1,maximum-scale=1,width=device-width,height=device-height,target-densitydpi=device-dpi,user-scalable=yes">
        <meta name="theme-color" content="#ffffff">
        {% endblock %}
        
        {% block css -%}
        <!-- Bootstrap -->
        <link rel="stylesheet" href="{{ asset('Resources/wrapkit/styles/bootstrap.css') }}">
        <!-- Bootstrap -->
        <!-- Dependencies -->
        <link rel="stylesheet" href="{{ asset('Resources/wrapkit/styles/dependencies.css') }}">
        <!-- endDependencies -->
        <!-- Wrapkit -->
        <link rel="stylesheet" href="{{ asset('Resources/wrapkit/styles/wrapkit.css') }}">
        <!-- endWrapkit -->

        <link rel="stylesheet" href="{{ asset('Resources/wrapkit/styles/components.css') }}">

        <link rel="stylesheet" href="{{ asset('Resources/ptTimeSelect/css/jquery.ptTimeSelect.css') }}">
        <link rel="stylesheet" href="{{ asset('Resources/zebra/css/bootstrap.css') }}">

        <!-- fullcalendar -->
        <link rel="stylesheet" href="{{ asset('bundles/adesignscalendar/css/fullcalendar/fullcalendar.css') }}" />
        <link rel="stylesheet" href="{{ asset('Resources/fullcalendar/css/fullcalendar.print.css') }}" media="print"/>
        <link rel="stylesheet" href="{{ asset('Resources/bValidator-0.73/themes/bvalidator.theme.bootstrap.rt.css') }}" />
        <!-- fullcalendar -->
        <style
            {# Hace los encabezados mas grandes#}
            .panel-title{
                font-size: 22px;
                font-weight: bold;
            }
            .panel-heading{
                
                font-size: 18px;
                font-weight: bold;
            }
            
                .panel-heading p{
                    margin-top:10px;
                }
            
            
        </style>
        {%- endblock %}
        
        
        
    {% endblock %}
    </head>
    <body {% block htmlbody %}{% endblock %}>
    {% block body -%}
        <main class="wrapkit-wrapper" id="wrapper">
        {% block bodycontenido -%}
            <!-- ============================================
            HEADER SECTION
            =============================================== -->
            {% block navigation -%}
            {% include 'DGPlusbelleBundle:Layout:navigation.html.twig'%} 
            {% endblock %}
            
            <!-- ============================================
            SIDEBAR SECTION
            =============================================== -->
            {% block sidebar %}
            {% include 'DGPlusbelleBundle:Layout:sidebar.html.twig'%}
            {% endblock %}
                    
            <section class="content-wrapper" role="main">
                <div class="content">
                <div class="content-bar">
                    <div class="pull-right" role="group">
                      {#<a data-toggle="modal" href="#" data-scripts="{{ asset('Resources/wrapkit/_includes/setups.js') }}" data-target="#templateSetup" title="Template Setups" aria-label="template setups" class="btn btn-nofill btn-sm btn-default" href="#"><span class="icon-settings fa-lg text-muted"></span></a>#}
                    </div>
                    {% block bar %}  
                    <ul class="breadcrumb breadcrumb-angle">
                        <li><a href="#" aria-label="home"><i class="fa fa-home"></i></a></li>
                        <li class="active">Dashboard</li>
                    </ul>
                    {% endblock %}   
                </div><!-- /.content-bar -->
                <div class="content-body">
                    {% block contenido %}{% endblock %} 
                </div><!-- /.content-body -->
                <!-- Template Setups -->
                <div class="modal fade" id="templateSetup">
                  <div class="modal-dialog">
                    <!-- modal-content -->
                    <div class="modal-content"></div>
                  </div><!-- /.modal-dialog -->
                </div><!-- /.templateSetup -->
                </div><!-- /.content -->
            </section><!-- /MAIN -->
        {% endblock %}
        {% block footer -%} 
        {% include 'DGPlusbelleBundle:Layout:footer.html.twig'%} 
        {% endblock %}
        </main>    
    {%- endblock %}  
                        

    {% block js -%}
